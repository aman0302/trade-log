from selenium import webdriver
from selenium.webdriver.common.keys import Keys

browser = webdriver.Firefox(executable_path=r'E:\code\geckodriver\geckodriver.exe')

browser.get('http://www.smallcase.com')
assert 'smallcase' in browser.title

elem = browser.find_element_by_id()
elem.send_keys('seleniumhq' + Keys.RETURN)

browser.quit()